# Payload CMS Environment Variables
# Copy this file to .env and fill in your values

# Your Neon PostgreSQL connection string (same as the main app)
DATABASE_URL=

# Secret key for Payload authentication (generate a random string)
PAYLOAD_SECRET=your-super-secret-key-change-me-in-production

# Optional: Server URL for media uploads
PAYLOAD_PUBLIC_SERVER_URL=http://localhost:3002
